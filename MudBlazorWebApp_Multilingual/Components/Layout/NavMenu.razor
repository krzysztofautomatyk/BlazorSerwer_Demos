@using Microsoft.Extensions.Localization
@inject IStringLocalizer<NavMenu> Localizer

<MudNavMenu>
    <MudNavLink Href="" Match="NavLinkMatch.All" Icon="@Icons.Material.Filled.Home">@Localizer["HomeLink"]</MudNavLink>
    <MudNavLink Href="counter" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.Add">@Localizer["CounterLink"]</MudNavLink>
    <MudNavLink Href="weather" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.List">@Localizer["WeatherLink"]</MudNavLink>
    <MudNavLink Href="settings" Match="NavLinkMatch.Prefix" Icon="@Icons.Material.Filled.Settings">@Localizer["SettingsLink"]</MudNavLink>
</MudNavMenu>


